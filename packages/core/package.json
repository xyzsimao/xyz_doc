{
  "name": "xyzdoc-core",
  "version": "16.4.6",
  "description": "The React.js library for building a documentation website",
  "keywords": [
    "Docs",
    "Fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "exports": {
    "./breadcrumb": {
      "types": "./dist/breadcrumb.d.ts",
      "import": "./dist/breadcrumb.js"
    },
    "./highlight": {
      "types": "./dist/highlight/index.d.ts",
      "import": "./dist/highlight/index.js"
    },
    "./highlight/client": {
      "types": "./dist/highlight/client.d.ts",
      "import": "./dist/highlight/client.js"
    },
    "./link": {
      "types": "./dist/link.d.ts",
      "import": "./dist/link.js"
    },
    "./source": {
      "types": "./dist/source/index.d.ts",
      "import": "./dist/source/index.js"
    },
    "./page-tree": {
      "types": "./dist/page-tree/index.d.ts",
      "import": "./dist/page-tree/index.js"
    },
    "./framework": {
      "types": "./dist/framework/index.d.ts",
      "import": "./dist/framework/index.js"
    },
    "./framework/*": {
      "types": "./dist/framework/*.d.ts",
      "import": "./dist/framework/*.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./next": "./dist/next.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.7.5",
    "@orama/orama": "^3.1.18",
    "@shikijs/rehype": "^3.21.0",
    "@shikijs/transformers": "^3.21.0",
    "estree-util-value-to-estree": "^3.5.0",
    "github-slugger": "^2.0.0",
    "hast-util-to-estree": "^3.1.3",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "fumadocs-core": "^16.4.6",
    "image-size": "^2.0.2",
    "negotiator": "^1.0.0",
    "npm-to-yarn": "^3.0.1",
    "path-to-regexp": "^8.3.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "scroll-into-view-if-needed": "^3.1.0",
    "shiki": "^3.21.0",
    "tinyglobby": "^0.2.15",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@mixedbread/sdk": "^0.50.0",
    "@orama/core": "^1.2.16",
    "@oramacloud/client": "^2.1.4",
    "@tanstack/react-router": "1.146.2",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/negotiator": "^0.6.4",
    "@types/node": "25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "algoliasearch": "5.46.2",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "lucide-react": "^0.562.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "mdast-util-mdxjs-esm": "^2.0.1",
    "next": "16.1.1",
    "react-router": "^7.12.0",
    "remark-directive": "^4.0.0",
    "remark-mdx": "^3.1.1",
    "remove-markdown": "^0.6.2",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "waku": "1.0.0-alpha.1",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@mixedbread/sdk": "^0.46.0",
    "@orama/core": "1.x.x",
    "@oramacloud/client": "2.x.x",
    "@tanstack/react-router": "1.x.x",
    "@types/react": "*",
